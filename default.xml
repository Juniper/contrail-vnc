<manifest>
<remote name="gerrit" fetch="https://review.opencontrail.org/Juniper"/>
<remote name="gerrittf" fetch="https://gerrit.tungsten.io/r/tungstenfabric"/>

<default revision="refs/heads/master" remote="github"/>

<project name="contrail-build" remote="gerrit" path="tools/build">
  <copyfile src="SConstruct" dest="SConstruct"/>
</project>
<project name="contrail-controller" remote="gerrit" path="controller"/>
<project name="contrail-vrouter" remote="gerrit" path="vrouter"/>
<project name="tf-third-party" remote="gerrit" path="third_party"/>
<project name="contrail-common" remote="gerrit" path="src/contrail-common"/>
<project name="tf-analytics" remote="gerrit" path="src/contrail-analytics"/>
<project name="contrail-api-client" remote="gerrit" path="src/contrail-api-client"/>
<project name="contrail-packages" remote="gerrit" path="tools/packages"/>
<project name="contrail-nova-vif-driver" remote="gerrit" path="openstack/nova_contrail_vif"/>
<project name="contrail-neutron-plugin" remote="gerrit" path="openstack/neutron_plugin"/>
<project name="contrail-heat" remote="gerrit" path="openstack/contrail-heat"/>
<project name="tf-web-controller" remote="gerrit" path="contrail-web-controller"/>
<project name="tf-web-core" remote="gerrit" path="contrail-web-core"/>
<project name="tf-webui-third-party" remote="gerrit" path="contrail-webui-third-party"/>
<project name="tf-tripleo-puppet" remote="gerrit" revision="stable/queens" path="openstack/contrail-tripleo-puppet"/>
<!-- vCenter projects -->
<project name="contrail-java-api" remote="gerrit" path="java-api"/>
<project name="vijava" remote="gerrit" path="vijava"/>
<project name="tf-vcenter-plugin" remote="gerrit" path="vcenter-plugin"/>
<project name="tf-vcenter-manager" remote="gerrit" path="vcenter-manager"/>
<project name="tf-vcenter-fabric-manager" remote="gerrit" path="vcenter-fabric-manager"/>
<project name="contrail-vrouter-java-api" remote="gerrit" path="vrouter-java-api"/>
<project name="contrail-vro-plugin" remote="gerrit" path="vro-plugin"/>
<!-- Used by OpenContrail CI jobs -->
<project name="contrail-dpdk" remote="gerrit" revision="contrail_dpdk_18_05_1" path="third_party/dpdk" />
<!-- contrail-specs repository contains per-branch directories, use master revision explicitly -->
<project name="contrail-specs" remote="gerrit" revision="master" path="contrail-specs" />
<project name="contrail-test" remote="gerrit" path="third_party/contrail-test" />
<!-- Contrail containers -->
<project name="contrail-container-builder" remote="gerrit" path="contrail-container-builder" />
<!-- Contrail deployers containers -->
<project name="contrail-deployers-containers" remote="gerrit" path="contrail-deployers-containers" />
<project name="contrail-helm-deployer" remote="gerrit" path="contrail-deployers-containers/containers/helm-deployer/src/contrail-helm-deployer" />
<project name="openstack-helm" remote="gerrit" path="contrail-deployers-containers/containers/helm-deployer/src/openstack-helm" />
<project name="openstack-helm-infra" remote="gerrit" path="contrail-deployers-containers/containers/helm-deployer/src/openstack-helm-infra" />
<project name="contrail-ansible-deployer" remote="gerrit" path="contrail-deployers-containers/containers/kolla-ansible-deployer/src/contrail-ansible-deployer" />
<project name="contrail-kolla-ansible" remote="gerrit" path="contrail-deployers-containers/containers/kolla-ansible-deployer/src/contrail-kolla-ansible" />
<project name="openshift-ansible" remote="gerrit" path="contrail-deployers-containers/containers/openshift-ansible-deployer/src/openshift-ansible" />
<project name="openshift-ansible" remote="gerrit" revision="release-3.7-contrail" path="contrail-deployers-containers/containers/openshift-ansible-deployer/src/openshift-ansible3.7" />
<project name="openshift-ansible" remote="gerrit" revision="release-3.9-contrail" path="contrail-deployers-containers/containers/openshift-ansible-deployer/src/openshift-ansible3.9" />
<project name="openshift-ansible" remote="gerrit" revision="release-3.11-contrail" path="contrail-deployers-containers/containers/openshift-ansible-deployer/src/openshift-ansible3.11" />
<!-- Contrail third party packages -->
<project name="contrail-third-party-packages" remote="gerrit" revision="master" path="third_party/contrail-third-party-packages" />

</manifest>

